{
  "name": "ui-form",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ui-form/src",
  "prefix": "cbc",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/ui-form"],
      "options": {
        "jestConfig": "libs/ui-form/jest.config.ts",
        "passWithNoTests": true,
        "tsConfig": "libs/ui-form/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/ui-form/src/**/*.ts",
          "libs/ui-form/src/**/*.html"
        ]
      }
    }
  }
}
